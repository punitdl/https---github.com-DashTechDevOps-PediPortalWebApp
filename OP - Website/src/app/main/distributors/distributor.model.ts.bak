import { FuseUtils } from '@fuse/utils';

export class Distributor
{
    id: string;
    name: string;
    lastName: string;
    avatar: string;
    nickname: string;
    company: string;
    jobTitle: string;
    email: string;
    phone: string;
    bphone: string;
    address: string;
    department: string;
    birthday: string;
    notes: string;
    team: string;
    team1: string;
    team2: string;
    team3: string;
    team4: string;
    dep1: string;
    dep2: string;
    dep3: string;
    dep4: string;
    reg1: string;
    reg2: string;
   reg3: string;
   reg4: string;
    region: string;
    zcode: string;
    city: string;
    state: string;
    cname: string;
    

    

    /**
     * Constructor
     *
     * @param distributor
     */
    constructor(distributor)
    {
        {
            this.id = distributor.id || FuseUtils.generateGUID();
            this.name = distributor.name || '';
            this.lastName = distributor.lastName || '';
            this.avatar = distributor.avatar || 'assets/images/avatars/profile.jpg';
            this.nickname = distributor.nickname || '';
            this.company = distributor.company || '';
            this.jobTitle = distributor.jobTitle || '';
            this.email = distributor.email || '';
            this.phone = distributor.phone || '';
            this.bphone = distributor.bphone || '';
            this.team = distributor.team || '';
            this.department = distributor.department || '';
            this.address = distributor.address || '';
            this.birthday = distributor.birthday || '';
            this.notes = distributor.notes || '';
            this.team1 = distributor.team1 || '';
            this.dep1 = distributor.dep1 || '';
            this.team2 = distributor.team2 || '';
            this.dep2 = distributor.dep2 || '';
            this.team3 = distributor.team3 || '';
            this.dep3 = distributor.dep3 || '';
            this.team4 = distributor.team4 || '';
            this.dep4 = distributor.dep4 || '';

            this.region = distributor.region || '';
            this.cname = distributor.cname || '';
            this.city = distributor.city || '';
            this.zcode = distributor.zcode || '';
            this.reg1 = distributor.reg1 || '';
            this.reg2 = distributor.reg2 || '';
            this.reg3 = distributor.reg3 || '';
            this.reg4 = distributor.reg4 || '';
            this.state = distributor.state || '';
           
        }
    }
}
